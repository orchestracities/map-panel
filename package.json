{
  "name": "map-panel",
  "version": "1.3.1",
  "description": "Map panel for grafana using traffic and environment data",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/city-of-data/dashboard/map-plugin.git"
  },
  "main": "src/module.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "lint": "eslint --color .",
    "lint:watch": "onchange 'src/**/*.js' -- npm run lint",
    "build": "grunt",
